(dyn) -> (stat dyn)

init_1:
	entry
	if dyn
		goto init_1_1_expl_1
		else statpath_1_1

init_1_1_expl_1:
	from init_1
	stat <- '0
	stat += dyn
	goto end_1_2

end_1_2:
	fi dyn
		from init_1_1_expl_1
		else merge_1_1_e1_2_expl_3
	stat += '3
	exit

statpath_1_1:
	from init_1
	if dyn
		goto left_1_1
		else right_1_1

left_1_1:
	from statpath_1_1
	assert(dyn)
	stat <- '3
	goto merge_1_1_e1_2_expl_3

merge_1_1_e1_2_expl_3:
	fi (stat = '3)
		from left_1_1
		else right_1_1
	goto end_1_2

right_1_1:
	from statpath_1_1
	assert(!(dyn))
	stat <- '9
	goto merge_1_1_e1_2_expl_3
